  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "tsup": "^8.4.0"
  },
  "packageManager": "pnpm@8.6.7",
  "publishConfig": {
    "access": "public"
  }
